{
  "$schema": "https://schemas.platformatic.dev/@platformatic/runtime/2.58.0.json",
  "services": [
    {
      "id": "main",
      "path": "main",
      "config": "platformatic.json"
    }
  ],
  "telemetry": {
    "enabled": true,
    "applicationName": "user-custom-name",
    "exporter": {
      "type": "otlp",
      "options": {
        "url": "http://jaeger:4318/v1/traces"
      }
    },
    "skip": [
      { "method": "GET", "path": "/health" }
    ]
  },
  "logger": {
    "level": "error"
  }
}
