{
  "$schema": "https://schemas.platformatic.dev/@platformatic/service/3.8.0.json",
  "server": {
    "logger": {
      "level": "silent"
    },
    "port": 3042,
    "keepAliveTimeout": 1
  },
  "service": {
    "openapi": true
  },
  "plugins": {
    "paths": ["./routes"]
  },
  "runtime": {
    "verticalScaler": {
      "enabled": true,
      "maxWorkers": 1
    }
  },
  "watch": true
}
